{
  "name": "Polybius",
  "version": "1.0",
  "description": "",
  "permissions": [
    "storage",
    "downloads"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": [
      "Polybius.js"
    ],
    "persistent": false
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ],
    "content_security_policy": "sandbox allow-scripts; script-src 'self'"
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "64": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "manifest_version": 2
}