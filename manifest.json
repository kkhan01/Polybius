{
  "name": "Polybius",
  "version": "1.0",
  "description": "",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "src/img/icon16.png",
      "32": "src/img/icon32.png",
      "64": "src/img/icon64.png",
      "128": "src/img/icon128.png"
    }
  },
  "icons": {
    "16": "src/img/icon16.png",
    "32": "src/img/icon32.png",
    "48": "src/img/icon64.png",
    "128": "src/img/icon128.png"
  },
  "manifest_version": 2
}